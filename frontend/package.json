    "husky": "^9",
    "lint-staged": "^15",
    "prettier": "^3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "jest": "^29",
    "ts-jest": "^29",
    "@types/jest": "^29",
    "@testing-library/react": "^16",
