{
    "name": "growset/module",
    "description": "Growset integration module",
    "type": "prestashop-module",
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Growset\\": "src/"
        }
    },
    "scripts": {
        "lint": "find . -path ./vendor -prune -o -name '*.php' -print0 | xargs -0 -n1 php -l",
        "test": [
            "@lint",
            "phpunit"
        ]
    }
}
